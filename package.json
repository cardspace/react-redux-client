{
  "name": "react-tutorials",
  "version": "0.0.0",
  "description": "",
  "main": "webpack.config.js",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {},
  "scripts": {
    
    "dev-clean": "rm -f src/dist/dev/*",
    "dev-static": "cp src/index.html src/dist/dev && cp src/card-space.css src/dist/dev",
    "dev-js": "webpack --config webpack.config.dev.js",
    "dev-build": "npm run dev-clean && npm run dev-static && npm run dev-js",
    "dev-server": "webpack-dev-server --content-base src/dist/dev --inline --hot --config webpack.config.dev.js",
    "dev": "npm run dev-build && npm run dev-server",    

    "stage-clean": "rm -f src/dist/stage/*",
    "stage-static": "cp src/index.html src/dist/stage && cp src/card-space.css src/dist/stage",
    "stage-js": "webpack --config webpack.config.stage.js",
    "stage-build": "npm run stage-clean && npm run stage-static && npm run stage-js", 
    "stage-server": "webpack-dev-server --content-base src/dist/stage --inline --hot --config webpack.config.stage.js",
    "stage": "npm run stage-build && npm run stage-server"

  },
  "author": "",
  "license": "ISC"
}
